   <menuitem id="menu_acs_settings" name="系統設定" parent="menu_acs"  action="action_acs_settings" />
    
    <record model="ir.ui.view" id="acs_settings">
      <field name="name">系統設定</field>
      <field name="model">acs.config.settings</field>
      <field name="arch" type="xml">
          <form string="設定">
                <header>
                      <!-- Buttons go here-->
                  </header>
                <sheet>
                <group>
                  <field name="deviceserver"/>
                </group>
                </sheet>
            
          </form>
      </field>
    </record>
        
        <menuitem id="menu_acs_accesscodelog" name="個人通關密碼設定紀錄" parent="menu_acs"  action="action_acs_accesscodelog" />

    <record model="ir.ui.view" id="acs_accesscodelog_list">
      <field name="name">個人通關密碼設定紀錄</field>
      <field name="model">acs.accesscodelog</field>
      <field name="arch" type="xml">
        <tree create="false" edit="false" delete="false" duplicate="false">
          <field name="user_role"/>
          <field name="user_id"/>
          <field name="user_name"/>
          <field name="devicegroup_name"/>
          <field name="create_time"/>
          <field name="expire_time"/>
          <field name="accesscode"/>
        </tree>
      </field>
    </record>
